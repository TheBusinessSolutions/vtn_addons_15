<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="action_report_stock_card_report_html_location" model="ir.actions.client">
        <field name="name">Stock Card Report Summary Location</field>
        <field name="tag">stock_card_report_backend</field>
        <field name="context" eval="{'active_model': 'report.stock.card.summary.location'}" />
    </record>

    <record id="action_stock_card_report_pdf_location" model="ir.actions.report">
        <field name="name">Stock Card PDF</field>
        <field name="model">report.stock.card.summary.location</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">aos_stock_card_summary_location.report_stock_card_report_pdf</field>
        <field name="report_file">aos_stock_card_summary_location.report_stock_card_report_pdf</field>
        <field
            name="print_report_name"
        >'STOCK CARD SUMMARY REPORT LOCATION'</field>
        <field name="paperformat_id" ref="aos_stock_card_summary_location.paperformat_stock_card_summary_location" />
    </record>

    <record id="action_stock_card_report_xlsx_location" model="ir.actions.report">
        <field name="name">STOCK CARD SUMMARY REPORT LOCATION</field>
        <field name="model">report.stock.card.summary.location</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">aos_stock_card_summary_location.stock_card_report_xlsx</field>
        <field name="report_file">Stock Card Report</field>
    </record>

</odoo>
